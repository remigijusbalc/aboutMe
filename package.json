{
  "name": "aboutMe",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "android": "react-native run-android",
    "ios": "cd ios && open aboutMe.xcodeproj",
    "f:start": "flow",
