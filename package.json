    "react-native-swiper": "^1.5.14",
    "react-native-vector-icons": "^6.3.0",
    "react-native-webview": "^5.3.0",
    "react-navigation": "^3.3.2",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "flow-bin": "0.86.0",
    "jest": "24.1.0",
    "metro-react-native-babel-preset": "0.52.0",
